{
    "manifest_version": 3,
    "name": "Everyday arXiv",
    "description": "Save and organize papers/paper URLs for reading later and EVERYDAY",
    "version": "0.5",
    "action": {
      "default_popup": "popup/popup.html"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    },
    "permissions": [
        "tabs",
        "storage",
        "unlimitedStorage"
    ]
    
  }